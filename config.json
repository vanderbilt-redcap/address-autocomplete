{
	"name": "Address Autocompletion",

	"namespace": "Vanderbilt\\AddressExternalModule",

	"description": "This module takes address coordinates from fields specified and autocompletes a street address. Requires a Google API key and an https connection.",

	"authors": [
	  {
		"name": "Scott J. Pearson",
		"email": "datacore@vumc.org",
